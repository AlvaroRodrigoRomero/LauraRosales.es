                                <div class="resume-section education">
                                    <div class="timeline-container">
                                        @foreach (var experience in Model.Experiences)
                                        {
                                            <div class="timeline-block wow @(experienceCount % 2 == 0 ? "fadeInLeft" : "fadeInRight")">
                                                <div class="timeline-img" data-anim-effect="zoomIn">
                                                    <i>@experience.Year</i>
                                                </div>

                                                <div class="timeline-content" data-anim-effect="@(experienceCount % 2 == 0 ? "fadeInLeft" : "fadeInRight")">
                                                    <h2>@experience.Title </h2>
                                                    <p>@experience.Restaurant </p>
                                                    <p>@Html.Raw(experience.Text) </p>
                                                </div>
                                            </div>
                                            experienceCount++;
                                        }
                                    </div>
                                </div>
                            </div>
                        </section>
                    }

