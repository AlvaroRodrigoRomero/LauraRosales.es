@model LadaWeb.Models.HomeModel
@using LadaWeb.Resources

@inject LocService SharedLocalizer
@{
    ViewData["Title"] = "Laura Rosales";

    var categoryCount = 0;
    var experienceCount = 0;
}
<div>
    <div class="responsive-menu">
        <a href="https://laurarosales.es"> <b>LAURA</b></a>
        <a id="menu-icon" class="but" href="#">
            <span class="ti-menu">
                <img src="https://cdn.laurarosales.es/images/menu.png" alt="image">
            </span>
        </a>
    </div>
    <div class="container-fluid">
        <div class="row">
            <div class="main scroll-page">
                <section id="home" class="home" style="background: url('@Model.Home.ImageBackgroundSrc'); background-attachment: fixed;background-position: center;
                        background-repeat: no-repeat;
                        background-size: cover;">
                    <div class="home-overlay"></div>
                    <div class="home-content" id="scroll-down">
                        <div class="home-intro">
                            <div class="row">
                                <div class="col-sm-3">
                                    <a class="logo" href="https://laurarosales.es"><img style="border: 3px solid #07cbf5;-moz-transform: scaleX(-1);-o-transform: scaleX(-1);-webkit-transform: scaleX(-1);transform: scaleX(-1); filter: FlipH; -ms-filter: " FlipH";" src="@Model.Home.ImageLogoSrc" alt="logo" width="260" height="260"></a>
                                </div>
                                <div class="col-sm-9">
                                    <h2>@Model.Home.H2</h2>
                                    <h1>@Model.Home.H1</h1>
                                    <div class="type-wrap">
                                        <div id="typed-strings">
                                            @foreach (var str in Model.Home.TypedStrings.Split(','))
                                            {
                                                <span>@str</span>
                                            }
                                        </div>
                                        <span id="typed" style="white-space:pre;"></span>
                                    </div>
                                    <!--<a class="download-link hvr-shutter-out-horizontal" href="">¿Descargar mi CV?</a>-->
                                </div>
                            </div>
                        </div>
                        <a class="scroll-down page-scroll" title="Scroll Down" href="#about"><i class="fa fa-circle" aria-hidden="true"></i></a>
                    </div>
                    <div class="menu_area" id="stick_menu">
                        <div class="container">
                            <div class="row">
                                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                                    <nav class="navbar navbar-default" role="navigation">
                                        <div class="container-fluid">
                                            <div class="navbar-collapse mainnavmenu" id="bs-example-navbar-collapse-1">
                                                <div id="menu-center">
                                                    <ul class="nav navbar-nav mainnav">

                                                        @foreach (var category in Model.Categories)
                                                        {
                                                            if ((bool)category.Active && categoryCount == 0)
                                                            {
                                                                <li class="active" active>
                                                                    <a href="#@category.Href" class="page-scroll">@category.Name <span class="sr-only">(current)</span></a>
                                                                </li>
                                                            }
                                                            else if ((bool)category.Active)
                                                            {
                                                                <li>
                                                                    <a href="#@category.Href" class="page-scroll">@category.Name </a>
                                                                </li>
                                                            }
                                                            categoryCount++;
                                                        }
                                                        <li>
                                                            @using (Html.BeginForm("Spanish", "Home", FormMethod.Post))
                                                            {
                                                                <button type="submit" style="display:contents">
                                                                    <img src="~/images/spanish.png" height="40" width="40" style="border-radius: 50%; margin-top: 5px; margin-left: 5px;" />
                                                                </button>
                                                            }
                                                        </li>
                                                        <li>
                                                            @using (Html.BeginForm("English", "Home", FormMethod.Post))
                                                            {
                                                                <button type="submit" style="display:contents">
                                                                    <img src="~/images/english.png" height="40" width="40" style="border-radius: 50%; margin-top: 5px; margin-left: 5px;" />
                                                                </button>
                                                            }
                                                        </li>
                                                    </ul>
